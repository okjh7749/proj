<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <Button
        android:id="@+id/normal_bt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="일반 게시판" />

    <Button
        android:id="@+id/img_bt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="이미지 게시판" />

    <Button
        android:id="@+id/found_bt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="찾아요 게시판" />
    <Button
        android:id="@+id/class_bt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="모여요 게시판" />
    <Button
        android:id="@+id/map_bt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="지도 게시판" />
    <Button
        android:id="@+id/ntag"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="일반게시판 태그 추천" />
    <Button
        android:id="@+id/itag"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="이미지 게시판 태그추천" />
</LinearLayout>